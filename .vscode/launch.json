{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH":"${workspaceFolder}",
                "CUDA_VISIBLE_DEVICES":"0"
            },
            "args": ["--mask-noisy=0.5",
                "--dataset=bwscifar10",
                "--sup-batch-size=100",
                "--usp-batch-size=100",
                "--label-exclude=False",
                "--num-labels=4000",
                "--arch=noisyhead2",
                "--model=inoisyheadmatch",
                "--threshold=.95",
                "--usp-weight=1.",
                "--ema-decay=0.97",
                "--optim=sgd",
                "--epochs=400",
                "--lr=0.1",
                "--momentum=0.9",
                "--weight-decay=5e-4",
                "--nesterov=True",
                "--lr-scheduler=cos",
                "--min-lr=1e-4",
                "--rampup-length=80",
                "--rampdown-length=50",
                "--data-twice=False",
                "--save-freq=0"],
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}